# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                   @controllers.ApplicationController.index
GET        /app                @controllers.ApplicationController.index
GET        /echo               @controllers.ApplicationController.echo
GET        /getJson            @controllers.ApplicationController.getJson
GET        /show               @controllers.ApplicationController.show(age: Int)
GET        /xml                @controllers.ApplicationController.xmlResult
GET        /html               @controllers.ApplicationController.html
GET        /futureHello        @controllers.ApplicationController.futureHello
